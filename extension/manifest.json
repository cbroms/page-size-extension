{
    "description": "Demonstrating webRequests",
    "manifest_version": 2,
    "name": "webRequest-demo",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["*://duckduckgo.com/*"],
            "js": ["content.js"]
        }
    ],

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "webNavigation",
        "<all_urls>"
    ],

    "background": {
        "scripts": ["background.js"]
    }
}
